{
    "name": "Google Classroom Assignment Fetcher",
    "displayName": "Google Classroom Assignment Fetcher",
    "description": "Fetch assignments from Google Classroom and save them to a local MongoDB database.",
    "version": "1.0.0",
    "publisher": "your-publisher-name",
    "engines": {
      "vscode": "^1.60.0"
    },
    "categories": [
      "Other"
    ],
    "activationEvents": [
      "onCommand:extension.fetchAssignments"
    ],
    "main": "./out/extension.js",
    "contributes": {
      "commands": [
        {
          "command": "extension.fetchAssignments",
          "title": "Fetch Assignments from Google Classroom"
        }
      ]
    },
    "scripts": {
      "vscode:prepublish": "npm run compile",
      "compile": "tsc -p ./"
    },
    "devDependencies": {
      "@types/node": "^14.14.37",
      "@types/vscode": "^1.60.0",
      "typescript": "^4.2.3",
      "tslint": "^6.1.3"
    },
    "dependencies": {
      "axios": "^0.21.1",
      "dotenv": "^8.2.0",
      "googleapis": "^39.2.0",
      "mongoose": "^5.10.9"
    }
  }